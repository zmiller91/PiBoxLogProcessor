{
    "name": "PBMessenger",
    "packageName": "com.zm.pbmessenger",

    "configuration": {
        "channels": {
            "test": "pblp_test",
            "prod": "pblp_prod"
        },
        "hosts": {
            "test": "localhost",
            "prod": "localhost"
        }
    },

    "api": {
        "name": "Api",
        "methods": [{
                "name": "log",
                "result": "GeneralLog",
                "params": [
                    {
                        "name": "message",
                        "ref": "GeneralLog"
                    }
                ]
            }]
    },

    "models": [
        {
            "name": "GeneralLog",
            "properties": [
                {
                    "name": "time",
                    "ref": "long"
                },
                {
                    "name": "id",
                    "ref": "string"
                },
                {
                    "name": "message",
                    "ref": "string"
                }
            ]
        },
        {
            "name": "string",
            "javaType": "String"
        },
        {
            "name": "long",
            "javaType": "long"
        }],

    "exceptions": []

}